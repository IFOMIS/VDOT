        <rdfs:label xml:lang="en">amsacrine</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/CHEBI_4031 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CHEBI_4031">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/CHEBI_16670"/>
        <obo:IAO_0000115 xml:lang="en">A cyclic nonribosomal peptide of eleven amino acids; an immunosuppressant drug widely used in post-allogeneic organ transplant to reduce the activity of the patient&apos;s immune system, and therefore the risk of organ rejection. Also causes reversible inhibition of immunocompetent lymphocytes in the G0- and G1-phase of the cell cycle.</obo:IAO_0000115>
        <obo:IAO_0000118 xml:lang="en">KEGG:D00184</obo:IAO_0000118>
        <obo:IAO_0000119 xml:lang="en">CHEBI</obo:IAO_0000119>
        <obo:IAO_0000412>http://purl.obolibrary.org/obo/CHEBI_4031</obo:IAO_0000412>
        <rdfs:comment>e.g. in Sandimmun optoral®</rdfs:comment>
        <rdfs:label xml:lang="de">Cyclosporin A</rdfs:label>
        <rdfs:label>cyclosporin A</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/CHEBI_4974 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CHEBI_4974">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/CHEBI_50860"/>
        <obo:IAO_0000115>2-Amino-9H-purine in which the hydrogen at position 9 is substituted by a 4-acetoxy-3-(acetoxymethyl)but-1-yl group. A prodrug of the antiviral penciclovir, it is used for the treatment of acute herpes zoster (shingles), for the treatment or suppression of recurrent genital herpes in immunocompetent patients and for the treatment of recurrent mucocutaneous herpes simplex infections in HIV infected patients</obo:IAO_0000115>
        <obo:IAO_0000118>KEGG:D00317</obo:IAO_0000118>
        <obo:IAO_0000119>CHEBI</obo:IAO_0000119>
        <obo:IAO_0000412>http://purl.obolibrary.org/obo/CHEBI_4974</obo:IAO_0000412>
        <rdfs:label>famciclovir</rdfs:label>
    </owl:Class>
    


